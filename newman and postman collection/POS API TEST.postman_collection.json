						{
							"name": "login-valid user",
							"event": [
								{
									"listen": "test",
									"script": {
										"exec": [
											"const json = pm.response.json();",
											"",
											"pm.test(\"200 OK\", () => {",
											"    pm.expect(pm.response.code).to.equal(200);",
											"});",
											"",
											"pm.test(\"Access token returned\", () => {",
											"    pm.expect(json).to.have.property(\"access_token\");",
											"    pm.environment.set(\"access_token\", json.access_token);",
											"});",
											"",
											"pm.test(\"User object valid\", () => {",
											"    pm.expect(json.user).to.have.property(\"id\");",
											"    pm.expect(json.user).to.have.property(\"username\");",
											"    pm.expect(json.user).to.have.property(\"role\");",
											"});",
											""
										],
										"type": "text/javascript",
										"packages": {}
									}
								}
							],
